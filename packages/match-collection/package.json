{
  "name": "@jack3898/match-collection",
  "description": "Run a smart matcher against a collection of items.",
  "version": "1.0.0",
  "author": "Jack Wright",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "nx": {
    "sourceRoot": "{projectRoot}/src",
    "projectType": "library",
    "targets": {
      "build": {
        "defaultConfiguration": "build"
      },
      "prepack": {
        "defaultConfiguration": "build"
      },
      "dev": {
        "defaultConfiguration": "dev"
      },
      "type-check": {
        "defaultConfiguration": "type-check"
      },
      "publish": {
        "defaultConfiguration": "publish"
      }
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./helpers": "./dist/helpers.js"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "files": [
    "./dist",
    "./LICENSE",
    "./README.md"
  ],
  "keywords": [
    "match",
    "collection",
    "matcher",
    "query",
    "ast",
    "utility",
    "tiny",
    "vanilla",
    "zero-dependency"
  ],
  "repository": "jack3898/jwtools.git",
  "types": "./dist/index.d.ts"
}
