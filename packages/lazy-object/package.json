{
  "name": "@jack3898/lazy-object",
  "description": "Lazily loaded objects with properties computed at get-time.",
  "version": "2.0.2",
  "author": "Jack Wright",
  "license": "Apache-2.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "nx": {
    "sourceRoot": "{projectRoot}/src",
    "projectType": "library",
    "targets": {
      "build": {
        "defaultConfiguration": "build"
      },
      "prepack": {
        "defaultConfiguration": "build"
      },
      "dev": {
        "defaultConfiguration": "dev"
      },
      "type-check": {
        "defaultConfiguration": "type-check"
      },
      "publish": {
        "defaultConfiguration": "publish"
      }
    }
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "files": [
    "./dist",
    "./LICENSE",
    "./README.md"
  ],
  "keywords": [
    "define-property",
    "esm",
    "getter",
    "inject",
    "lazy",
    "lazy-evaluation",
    "memo",
    "object",
    "object-literal",
    "performance",
    "persist",
    "tiny",
    "utility",
    "vanilla"
  ],
  "repository": "jack3898/jwtools.git",
  "types": "./dist/index.d.ts"
}
